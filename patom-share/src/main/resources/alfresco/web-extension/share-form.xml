<alfresco-config>

   <!-- cm:content type (existing nodes) -->
    <config evaluator="node-type" condition="pt:document">
        <forms>
            <form>
                <field-visibility>
<!--                <show id="pt:documentId"/>-->
<!--                <show id="pt:signCity"/>-->
                <show id="pt:signDate"/>
                <show id="pt:farmOwnerFirstName"/>
                <show id="pt:farmOwnerSurname"/>
                <show id="pt:farmOwnerNIP"/>
                <show id="pt:farmZipCode"/>
                <show id="pt:farmCity"/>
                <show id="pt:farmStreet"/>
                <show id="pt:farmId"/>
                <show id="pt:farmBreedingGroundAddress"/>
                <show id="pt:farmBreedingGroundName"/>
                <show id="pt:farmBreedingGroundId"/>
                <show id="pt:ifHerdWellBeingProvided"/>
                <show id="pt:herdRace"/>
                <show id="pt:herdQuantity"/>
                <show id="pt:herdDeploymenDate"/>
                <show id="pt:herdMedicals"/>
                <show id="pt:herdMedicalsTreatmentDates"/>
                <show id="pt:herdMedicalsApplyingDatesFrom"/>
                <show id="pt:herdMedicalsApplyingDatesTo"/>
                <show id="pt:wetSurname"/>
                <show id="pt:wetCity"/>
                <show id="pt:wetPhoneNumber"/>
                <show id="pt:ifAnyMedicalIssues"/>
                <show id="pt:treatRace"/>
                <show id="pt:historicIssues"/>
                <show id="pt:historicDiseaseDurationTo"/>
                <show id="pt:ifAnyMedicalIssuesInButcheredHerds"/>
                <show id="pt:historicDiseaseInButcheredHerds"/>
                <show id="pt:starterFeedTimeFrom"/>
                <show id="pt:starterFeedTimeTo"/>
                <show id="pt:growerFeedTimeFrom"/>
                <show id="pt:growerFeedTimeTo"/>
                <show id="pt:finisherFeedTimeFrom"/>
                <show id="pt:finisherFeedTimeTo"/>
                <show id="pt:starterName"/>
                <show id="pt:growerName"/>
                <show id="pt:finisherName"/>
                <show id="pt:foodSource"/>
                </field-visibility>
                <appearance>
                    <field id="pt:signDate">
                        <control template="/org/alfresco/components/form/controls/date.ftl">
                            <control-param name="showTime">false</control-param>
                        </control>
                    </field>
                    <field id="pt:farmOwnerFirstName">
                        <control template="/org/alfresco/components/form/controls/selectone.ftl">
                            <control-param name="options">
                               -1|Patrycja,1|Tomek
                            </control-param>
                        </control>
                    </field>
                    <field id="pt:farmOwnerSurname">
                        <control template="/org/alfresco/components/form/controls/selectone.ftl">
                            <control-param name="options">
                               -1|Pietryka,1|Kowalski
                            </control-param>
                        </control>
                    </field>
                    <field id="pt:farmOwnerNIP">
                        <control template="/org/alfresco/components/form/controls/selectone.ftl">
                            <control-param name="options">
                               -1|12334456,1|98765434,2|766545678,3|9998865534
                            </control-param>
                        </control>
                    </field>
                    <field id="pt:farmZipCode">
                        <control template="/org/alfresco/components/form/controls/selectone.ftl">
                            <control-param name="options">
                               -1|64-310,1|64-311|64-987
                            </control-param>
                        </control>
                    </field>
                    <field id="pt:farmCity">
                        <control template="/org/alfresco/components/form/controls/selectone.ftl">
                            <control-param name="options">
                               -1|Lwówek,1|Posadowo,2|Józefowo
                            </control-param>
                        </control>
                    </field>
                    <field id="pt:farmStreet">
                        <control template="/org/alfresco/components/form/controls/selectone.ftl">
                            <control-param name="options">
                               -1|Al. Sczanieckiej 34,1|Posadowska 2,2|Muska 13/3
                            </control-param>
                        </control>
                    </field>
                    <field id="pt:farmId">
                        <control template="/org/alfresco/components/form/controls/selectone.ftl">
                            <control-param name="options">
                               -1|1,1|2,2|3,3|4,4|5
                            </control-param>
                        </control>
                    </field>
                    <field id="pt:farmBreedingGroundAddress">
                        <control template="/org/alfresco/components/form/controls/selectone.ftl">
                            <control-param name="options">
                               -1|Al. Sczanieckiej 34,1|Posadowska 2,2|Muska 13/3
                            </control-param>
                        </control>
                    </field>
                    <field id="pt:farmBreedingGroundName">
                        <control template="/org/alfresco/components/form/controls/selectone.ftl">
                            <control-param name="options">
                               -1|Chicken Slayer,1|Killing Floor,2|Chicken Heaven
                            </control-param>
                        </control>
                    </field>
                    <field id="pt:farmBreedingGroundId">
                        <control template="/org/alfresco/components/form/controls/selectone.ftl">
                            <control-param name="options">
                               -1|12-HY,1|34-PO,2|21-XX
                            </control-param>
                        </control>
                    </field>
                    <field id="pt:ifHerdWellBeingProvided">
                        <control template="/org/alfresco/components/form/controls/checkbox.ftl" />
                    </field>
                    <field id="pt:herdRace">
                        <control template="/org/alfresco/components/form/controls/textfield.ftl" />
                    </field>
                    <field id="pt:herdQuantity">
                        <control template="/org/alfresco/components/form/controls/textfield.ftl" />
                    </field>
                    <field id="pt:herdDeploymenDate">
                        <control template="/org/alfresco/components/form/controls/date.ftl">
                            <control-param name="showTime">false</control-param>
                        </control>
                    </field>
                    <field id="pt:herdMedicals">
                        <control template="/org/alfresco/components/form/controls/textfield.ftl" />
                    </field>
                    <field id="pt:herdMedicalsTreatmentDates">
                        <control template="/org/alfresco/components/form/controls/date.ftl">
                            <control-param name="showTime">false</control-param>
                        </control>
                    </field>
                    <field id="pt:herdMedicalsApplyingDatesFrom">
                        <control template="/org/alfresco/components/form/controls/date.ftl">
                            <control-param name="showTime">false</control-param>
                        </control>
                    </field>
                    <field id="pt:herdMedicalsApplyingDatesTo">
                        <control template="/org/alfresco/components/form/controls/date.ftl">
                            <control-param name="showTime">false</control-param>
                        </control>
                    </field>
                    <field id="pt:wetSurname">
                        <control template="/org/alfresco/components/form/controls/textfield.ftl" />
                    </field>
                    <field id="pt:wetCity">
                        <control template="/org/alfresco/components/form/controls/textfield.ftl" />
                    </field>
                    <field id="pt:wetPhoneNumber">
                        <control template="/org/alfresco/components/form/controls/textfield.ftl" />
                    </field>
                    <field id="pt:ifAnyMedicalIssues">
                        <control template="/org/alfresco/components/form/controls/checkbox.ftl" />
                    </field>
                    <field id="pt:treatRace">
                        <control template="/org/alfresco/components/form/controls/textfield.ftl" />
                    </field>
                    <field id="pt:historicIssues">
                        <control template="/org/alfresco/components/form/controls/textfield.ftl" />
                    </field>
                    <field id="pt:historicDiseaseDurationTo">
                        <control template="/org/alfresco/components/form/controls/date.ftl">
                            <control-param name="showTime">false</control-param>
                        </control>
                    </field>
                    <field id="pt:ifAnyMedicalIssuesInButcheredHerds">
                        <control template="/org/alfresco/components/form/controls/checkbox.ftl" />
                    </field>
                    <field id="pt:historicDiseaseInButcheredHerds">
                        <control template="/org/alfresco/components/form/controls/textfield.ftl" />
                    </field>
                    <field id="pt:starterFeedTimeFrom">
                        <control template="/org/alfresco/components/form/controls/date.ftl">
                            <control-param name="showTime">false</control-param>
                        </control>
                    </field>
                    <field id="pt:starterFeedTimeTo">
                        <control template="/org/alfresco/components/form/controls/date.ftl">
                            <control-param name="showTime">false</control-param>
                        </control>
                    </field>
                    <field id="pt:growerFeedTimeFrom">
                        <control template="/org/alfresco/components/form/controls/date.ftl">
                            <control-param name="showTime">false</control-param>
                        </control>
                    </field>
                    <field id="pt:growerFeedTimeTo">
                        <control template="/org/alfresco/components/form/controls/date.ftl">
                            <control-param name="showTime">false</control-param>
                        </control>
                    </field>
                    <field id="pt:finisherFeedTimeFrom">
                        <control template="/org/alfresco/components/form/controls/date.ftl">
                            <control-param name="showTime">false</control-param>
                        </control>
                    </field>
                    <field id="pt:finisherFeedTimeTo">
                        <control template="/org/alfresco/components/form/controls/date.ftl">
                            <control-param name="showTime">false</control-param>
                        </control>
                    </field>
                    <field id="pt:starterName">
                        <control template="/org/alfresco/components/form/controls/textfield.ftl" />
                    </field>
                    <field id="pt:growerName">
                        <control template="/org/alfresco/components/form/controls/textfield.ftl" />
                    </field>
                    <field id="pt:finisherName">
                        <control template="/org/alfresco/components/form/controls/textfield.ftl" />
                    </field>
                    <field id="pt:foodSource">
                        <control template="/org/alfresco/components/form/controls/selectone.ftl">
                            <control-param name="options">
                               -1|Własna,1|Z zakupu
                            </control-param>
                        </control>
                    </field>
                </appearance>
            </form>
        </forms>
    </config>
</alfresco-config>
